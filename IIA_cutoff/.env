OPENAI_API_KEY= #put the API key here
OPENAI_MODEL=gpt-4o-mini

# ---- Pipeline switches ----
IIA_RESET_DW=1
IIA_RUN_GENERATORS=1
IIA_START_ETL=0
IIA_MODE=db

# ---- Data Warehouse ----
IIA_DW_PATH=data_warehouse.db
IIA_ARTIFACT_DIR=artifacts
IIA_BILL_THRESHOLD=600
IIA_MIN_CLIENT_ROWS=5

# ---- Mongo (YOUR REAL ONE) ----
IIA_MONGO_URI=mongodb://localhost:27017
IIA_MONGO_DB=MedicalRecords
IIA_MONGO_PATIENTS_COLL=Patients

# ---- Redis ----
IIA_REDIS_HOST=localhost
IIA_REDIS_PORT=6379
IIA_REDIS_DB=0
IIA_REDIS_VITALS_PREFIX=vitals:

# ---- Postgres (YOUR REAL ONE) ----
IIA_PG_HOST=localhost
IIA_PG_PORT=5432
IIA_PG_USER=postgres
IIA_PG_PASS=admin
IIA_PG_DB=postgres

# ---- SQLite SOURCE (billing DB created by generator) ----
IIA_SQLITE_SOURCE_PATH=BillingRecords.db
IIA_SQLITE_BILLING_TABLE=Billing